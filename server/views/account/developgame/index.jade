          |setLevelText(3, "Get 20% of the Euglena on the screen into the moving blue box at any given moment in time. The blue box will randomly move around the screen.");
          |&nbsp;
          |setGameOverMessage("Game over, yo!");
          |&nbsp;
          |if (true) {
          |&nbsp;
