        br

        |&nbsp;&nbsp;&nbsp;&nbsp;public void startGame() {
        br
        |&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        textarea#txtCodeStart(rows='6', cols='55', disabled)
          |Under construction.
        br
        |&nbsp;&nbsp;&nbsp;&nbsp;}
        br
        |&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        button.btn.btn-success#btnUpdateStart(type="update",value="update", disabled) Update Code
        br
        br

        |&nbsp;&nbsp;&nbsp;&nbsp;public void endGame() {
        br
        |&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        textarea#txtCodeEnd(rows='6', cols='55', disabled)
          |Under construction.
        br
        |&nbsp;&nbsp;&nbsp;&nbsp;}
        br
        |&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        button.btn.btn-success#btnUpdateEnd(type="update",value="update", disabled) Update Code
        br
        br

        |&nbsp;&nbsp;&nbsp;&nbsp;public void onJoystickChange(int angle, int intensity) {
        br
        |&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        textarea#txtCodeJoystick(rows='6', cols='55', disabled)
          |Under construction.
        br
        |&nbsp;&nbsp;&nbsp;&nbsp;}
        br
        |&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        button.btn.btn-success#btnUpdateJoystick(type="update",value="update", disabled) Update Code
        br
        br

        |}
      div.col-xs-5
        //img#myVideo.unselectable(src='http://fay.iniminimo.com/shapes/giftwrap/giftwrap01.gif')
        //(src='#{data.mainImageIP}')
        //br
        div.row
          div.col-xs-5#myAside.unselectable(name='myAside')
            div#myJoystick.canvas-joystick-off(name='myJoystick')
          div.col-xs-1
          div.col-xs-6
            |<u>Level 
            b#level
              |1
            |: </u>
            p#levelText
              |Get 10% of the Euglena on the screen into the moving blue box at any given moment in time. The blue box will randomly move around the screen.
        br
        canvas(id="display", width="640", height="480", hidden="true")
        canvas(id="processed", width="640", height="480")
        h2
          |Status 
          code(id="statusField")
            |Loading...
    
    div.modal.fade(role="dialog")

    script(type='text/template',id="tmpl-survey")
      div.modal-dialog(role="document")
        div.modal-content
          div.modal-header
              h4.modal-title Send us your feedback
          div.modal-body
            .form-group
                label Rate your experience
                div
                    input.form-control.rating(type="hidden",name="rating")
            .form-group
                label Note for support team
                textarea.form-control(rows="5",style="width:520px", name="notes")
          div.modal-footer
            button.btn.btn-primary#btn-submit(type="submit",value="submit") Submit  
            button.btn.btn-default#btn-close(type="button",value="close", data-dismiss="modal") Close  
      

    script(type='text/lab', id='data-user') !{data.user}
    script(type='text/lab', id='data-bpu') !{data.bpu}
    script(type='text/lab', id='data-bpuExp') !{data.bpuExp}
    script(type='text/template', id='data-session') !{data.session}
    script(type='text/lab', id='data-setLedsObj') !{data.setLedsObj}

