        br
        |&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        br

        |&nbsp;&nbsp;&nbsp;&nbsp;
        font(color='#0F6BA0')
          |this
        |.startGame = 
        font(color='purple')
          |function
        |() {
        br
        |&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        div.row
          div.col-xs-1
          div.col-xs-11
            textarea.codeArea#txtCodeStart
