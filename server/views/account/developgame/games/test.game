this.currLED = LED.RIGHT;
this.possibleLEDs = [LED.UP, LED.DOWN, LED.RIGHT, LED.LEFT];
this.instructions = "Guess which LED is currently shining. Press 'W'" +
    "for top LED, 'A' for left LED, 'S' for bottom LED, and" +
    "'D' for right LED.";
this.score = 1;
-----
setLED(this.currLED, 999);
if (this.score >= 10) {
    finishGame();
}
-----
setJoystickView(false);
setLevel(1);
setTextPerLevel(1, this.instructions);
-----
alert('Guess correctly 10 times, congrats!');
-----
//console.log('Joystick changed!');
-----
switch(key) {
    case KEY.W:
        if (this.currLED == LED.UP) {
            alert('Correct!');
            this.score += 1;
            this.currLED = this.possibleLEDs[Math.floor(Math.random() * this.possibleLEDs.length)];
        } else {
            alert('Wrong!');
        }
        break;
    case KEY.A:
        if (this.currLED == LED.LEFT) {
            alert('Correct!');
            this.score += 1;
            this.currLED = this.possibleLEDs[Math.floor(Math.random() * this.possibleLEDs.length)];
        } else {
            alert('Wrong!');
        }
        break;
    case KEY.S:
        if (this.currLED == LED.DOWN) {
            alert('Correct!');
            this.score += 1;
            this.currLED = this.possibleLEDs[Math.floor(Math.random() * this.possibleLEDs.length)];
        } else {
            alert('Wrong!');
        }
        break;
    case KEY.D:
        if (this.currLED == LED.RIGHT) {
            alert('Correct!');
            this.score += 1;
            this.currLED = this.possibleLEDs[Math.floor(Math.random() * this.possibleLEDs.length)];
        } else {
            alert('Wrong!');
        }
        break;
    default:
        break;
}